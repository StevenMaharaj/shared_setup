alias sb="source ~/.bashrc"
alias sv="source .venv/bin/activate"
alias dv="deactivate"

alias pts="poetry shell"
alias pti="poetry init"
alias pta="poetry add"
alias pte="poetry env use ~/anaconda3/envs/py3.12/bin/python"

alias gp="git pull"
alias gs="git status"
alias ga="git add ."
alias gc="git commit -m"
alias pycharm="sh /opt/pycharm-community-2024.2.4/bin/pycharm.sh"

alias dasp="django-admin startproject"
alias dasa="django-admin startapp"

alias dmrs="python manage.py runserver"
alias dmmm="python manage.py makemigrations"
alias dms="python manage.py shell"
alias dmmi="python manage.py migrate"
alias dmcs="python manage.py createsuperuser"
